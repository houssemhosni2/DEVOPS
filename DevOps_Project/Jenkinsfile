pipeline {
    agent any 
    environment {
        JAVA_HOME = '/usr/lib/jvm/java-17-openjdk-amd64'
        M2_HOME = '/usr/share/maven'
        PATH = "${M2_HOME}/bin:${PATH}"
    }
stages {
            stage('Checkout') {
            steps {
              git branch: 'ahmed2',
              url: "https://github.com/zankech/project.git"
            }
        }

    stage('Build') {
        steps {
            dir('DevOps_Project') {
                sh 'mvn clean package'
            }
        }
    }

        }
}
