pipeline {
    agent any
    stages {
            stage('Checkout') {
            steps {
              git branch: 'ahmed2',
              url: "https://github.com/devopsgroup24-25/devops-24-25.git"
            }
        }

    stage('Build') {
        steps {
            dir('DevOps_Project') {
                sh 'mvn clean package'
            }
        }
    }

        }
}
